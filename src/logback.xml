<configuration>

	<appender name="FILE" class="ch.qos.logback.core.FileAppender">
		<file>c:/Smsmanager/LOG.log</file>
		<append>true</append>
		<encoder>
			<pattern>[%d] [%thread] %-5level %logger{36} - %msg%n</pattern>
		</encoder>

	</appender>

	<root level="debug">
		<appender-ref ref="FILE" />
	</root>
</configuration>